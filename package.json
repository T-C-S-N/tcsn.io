{
  "name": "tcsn.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "tcsn",
    "email": "hello@tcsn.io"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "pages:dev": "wrangler pages dev dist --compatibility-date=2024-01-01",
    "pages:build": "vite build",
    "pages:deploy": "pnpm run pages:build && wrangler pages deploy dist --project-name=tcsnio",
    "pages:deploy:prod": "pnpm run pages:build && wrangler pages deploy dist --project-name=tcsnio --env production",
    "worker:dev": "wrangler dev workers/index.js",
    "worker:deploy": "wrangler deploy workers/index.js --name tcsnio",
    "worker:deploy:prod": "wrangler deploy workers/index.js --name tcsnio --env production",
    "deploy:all": "pnpm run worker:deploy:prod && pnpm run pages:deploy:prod",
    "d1:create": "wrangler d1 create tcsnio",
    "d1:query": "wrangler d1 execute tcsnio --command",
    "d1:migrations:create": "wrangler d1 migrations create tcsnio"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.0",
    "@fortawesome/free-regular-svg-icons": "^6.5.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.0",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "axios": "^1.6.0",
    "dotenv": "^16.6.1",
    "google-auth-library": "^10.3.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.294.0",
    "moment": "^2.29.4",
    "openai": "^5.20.0",
    "pinia": "^2.1.0",
    "prismjs": "^1.29.0",
    "three": "^0.158.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.31",
    "sass": "^1.69.0",
    "tailwindcss": "^3.3.0",
    "vite": "^5.4.0",
    "wrangler": "^3.80.0"
  }
}
